{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/wiki",
      "destination": "/wiki/index.html"
    },
    {
      "source": "/wiki/:path*",
      "destination": "/wiki/:path*"
    }
  ],
  "routes": [
    {
      "src": "/app",
      "dest": "/PWA/index.html"
    },
    {
      "src": "/app/(.*)",
      "dest": "/PWA/$1"
    }
  ]
}
